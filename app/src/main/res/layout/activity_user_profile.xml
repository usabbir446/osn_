        android:layout_width="70dp"
        android:layout_height="68dp"
        android:background="@drawable/shapeleftcorner"
        android:padding="5dp"
        app:layout_constraintBottom_toTopOf="@id/guideline13"
        app:layout_constraintEnd_toStartOf="@+id/avataru"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/guideline12"
        app:srcCompat="@drawable/foldersmall" />

    <ImageView
        android:id="@+id/avataru"
        android:layout_width="70dp"
        android:layout_height="68dp"
        android:background="@drawable/shapesquare"
        android:padding="8dp"
        app:layout_constraintBottom_toTopOf="@id/guideline13"
        app:layout_constraintEnd_toStartOf="@+id/scan"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toEndOf="@id/directory"
        app:layout_constraintTop_toBottomOf="@id/guideline12"
        app:srcCompat="@drawable/avatarsmallround" />

    <ImageView
        android:id="@+id/edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="520dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/notification"
        app:layout_constraintStart_toEndOf="@+id/helpline"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/edit" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".055" />

